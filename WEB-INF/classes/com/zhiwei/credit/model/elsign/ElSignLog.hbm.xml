<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.elsign.ElSignLog"
        table="el_sign_log"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="contractId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="contractId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="businessType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="businessType"
            length="32"
            not-null="false"
            unique="false"
        />
        <property
            name="memType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="memType"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="memId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="memId"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="isSign"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isSign"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="createtime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createtime"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>

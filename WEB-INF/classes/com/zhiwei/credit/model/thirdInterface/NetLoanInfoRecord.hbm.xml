<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.zhiwei.credit.model.thirdInterface.NetLoanInfoRecord"
        table="net_loaninfo_record"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="recordId"
            column="recordId"
            type="java.lang.Long"
            length="11"
        >
            <generator class="native"/>
        </id>
         <property
            name="accid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="accid"
            length="11"
            not-null="false"
            unique="false"
        />
         <property
            name="accname"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="memberName"
            length="20"
            not-null="false"
            unique="false"
        />
          <property
            name="acfreebal"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="acfreebal"
            length="11"
            not-null="false"
            unique="false"
        />
          <property
            name="usableamount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="usableamount"
            length="11"
            not-null="false"
            unique="false"
        />
          <property
            name="freezeamount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="freezeamount"
            length="11"
            not-null="false"
            unique="false"
        />
        <property
            name="riskamount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="riskamount"
            length="11"
            not-null="false"
            unique="false"
        />
        <property
            name="queryTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="queryTime"
             length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="orderNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="orderNo"
            length="50"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
